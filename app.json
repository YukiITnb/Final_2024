{
  "expo": {
    "name": "Habit Tracker",
    "slug": "habitapp",
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splashscr.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.yukiexpo.habitapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-asset",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "The app needs permission to access your photos to upload images."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ca27e114-a5a3-4112-aff0-37611f18173a"
      }
    },
    "owner": "yukiexpo",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ca27e114-a5a3-4112-aff0-37611f18173a"
    }
  }
}
